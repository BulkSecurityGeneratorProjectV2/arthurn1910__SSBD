<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale>pl</default-locale>
            <supported-locale>pl</supported-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>i18n.messages</base-name>
            <var>msg</var>
        </resource-bundle>
        <message-bundle>
            i18n.jsf_messages
        </message-bundle>
    </application>  
    <navigation-rule>   
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/success.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/index.html</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>errorLogin</from-outcome>
            <to-view-id>/mok/errorLogin.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>edytujDane</from-outcome>
            <to-view-id>/mok/edytujDane.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>zalogowano</from-outcome>
            <to-view-id>/mok/zalogowano.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>edytujDaneKontaInnegoUzytkownika</from-outcome>
            <to-view-id>/mok/edytujDaneKontaInnegoUzytkownika.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>wyswietlInformacje</from-outcome>
            <to-view-id>/mok/wyswietlInformacje.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>zmienHasloUzytkownika</from-outcome>
            <to-view-id>/mok/zmienHasloUzytkownika.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>odlaczPoziom</from-outcome>
            <to-view-id>/mok/odlaczPoziom.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>dolaczPoziom</from-outcome>
            <to-view-id>/mok/dolaczPoziom.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mok/zarzadzanieKontami.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>kontaNiepotwierdzone</from-outcome>
            <to-view-id>/mok/kontaNiepotwierdzone.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>wyszukajKonta</from-outcome>
            <to-view-id>/mok/parametryWyszukiwaniaUzytkownika.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>utworzKonto</from-outcome>
            <to-view-id>/mok/utworzKonto.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mok/kontaNiepotwierdzone.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>powrot</from-outcome>
            <to-view-id>/mok/zarzadzanieKontami.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mok/parametryWyszukiwaniaUzytkownika.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>powrot</from-outcome>
            <to-view-id>/mok/zarzadzanieKontami.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mok/utworzKonto.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>powrot</from-outcome>
            <to-view-id>/mok/zarzadzanieKontami.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>zmienWlasneHaslo</from-outcome>
            <to-view-id>/mok/zmienWlasneHaslo.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
</faces-config>
